{
  "name": "jsonfixer",
  "version": "0.1.0",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "dev": "node start-dev.js",
    "fresh": "node -e \"try{require('fs').rmSync('.next',{recursive:1,force:1})}catch(e){}\" && node start-dev.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest --run",
    "electron": "npm run build && electron .",
    "electron-build": "npm run build && electron-builder"
  },
  "build": {
    "appId": "com.jsonfixer.app",
    "productName": "JSONFixer",
    "files": [
      "electron.js",
      ".next/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtualized-auto-sizer": "^2.0.2",
    "react-window": "^2.2.6"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.24",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "fast-check": "^4.5.3",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
